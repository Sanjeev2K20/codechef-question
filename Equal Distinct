Question: https://www.codechef.com/submit/EQDIS
code: https://onlinegdb.com/5nkKkjJq0_

#include<iostream>
#include<algorithm>
#include<set>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int arr[n];
        set<int>s;
        for(int i=0;i<n;i++){
            cin>>arr[i];
            s.insert(arr[i]);
        }
        int m=s.size();
        if(n%2!=0 && m%2!=0 && m==n){
            cout<<"NO"<<endl;
        }
        else{
            cout<<"YES"<<endl;
        }
        
    }
}
